{
  "name": "svgson-next",
  "version": "4.1.0",
  "description": "svgson next generation",
  "main": "dist/svgson.cjs.js",
  "module": "dist/svgson.esm.js",
  "browser": "dist/svgson.umd.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "ava",
    "pretest": "npm run build",
    "cover": "nyc --reporter=lcov --reporter=text npm t",
    "report-coverage": "cat ./coverage/lcov.info | codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elrumordelaluz/svgson-next.git"
  },
  "keywords": [
    "svg",
    "json",
    "svgo"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Lionel T <elrumordelaluz@hotmail.com> (https://elrumordelaluz.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elrumordelaluz/svgson-next/issues"
  },
  "homepage": "https://github.com/elrumordelaluz/svgson-next#readme",
  "dependencies": {
    "clean-deep": "3.0.2",
    "deep-rename-keys": "^0.2.1",
    "omit-deep": "0.3.0",
    "xml-printer": "1.1.3",
    "xml-reader": "2.4.3"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "4.1.2",
    "codecov.io": "0.1.6",
    "nyc": "11.8.0",
    "rollup": "0.59.4",
    "rollup-plugin-babel": "3.0.4",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-node-resolve": "3.3.0",
    "svgo": "1.0.5",
    "svgson": "2.1.1"
  },
  "nyc": {
    "lines": 90,
    "statements": 90,
    "functions": 70,
    "branches": 70,
    "check-coverage": true,
    "exclude": [
      "**/*.test.js"
    ]
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-2"
    ]
  },
  "xo": {
    "esnext": true
  }
}
