{
  "name": "svgson-next",
  "version": "2.2.0",
  "description": "Experiments for svgson",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test:watch": "npm t -- -w",
    "test": "mocha src/*.test.js --require babel-register",
    "cover": "nyc --reporter=lcov --reporter=text npm t",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --out-dir dist --ignore *.test.js src",
    "build:umd": "webpack --output-filename index.umd.js --mode production",
    "build:umd.min": "webpack --output-filename index.umd.min.js --mode production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elrumordelaluz/svgson-next.git"
  },
  "keywords": [
    "svg",
    "json",
    "svgo"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Lionel T <elrumordelaluz@hotmail.com> (https://elrumordelaluz.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elrumordelaluz/svgson-next/issues"
  },
  "homepage": "https://github.com/elrumordelaluz/svgson-next#readme",
  "dependencies": {
    "clean-deep": "3.0.2",
    "deep-rename-keys": "^0.2.1",
    "dom-serializer": "^0.1.0",
    "htmlparser2": "3.9.2",
    "omit-deep": "0.3.0",
    "svgo": "1.0.5"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "ghooks": "2.0.3",
    "mocha": "5.1.1",
    "npm-run-all": "4.1.2",
    "nyc": "11.7.1",
    "rimraf": "2.6.2",
    "semantic-release": "15.1.7",
    "svgson": "2.1.1",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.14"
  },
  "config": {
    "ghooks": {},
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "check-coverage": true,
    "exclude": [
      "**/*.test.js"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
